{
  "name": "slowreader-vscode",
  "displayName": "SlowReader",
  "description": "A VSCode extension that reveals text one character at a time, simulating a typewriter-style animation.",
  "version": "0.1.0",
  "author": "walkingshamrock",
  "license": "MIT",
  "engines": {
    "vscode": "^1.85.0"
  },
  "categories": [
    "Other"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "slowreader.start",
        "title": "SlowReader: Start Typing Animation"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.85.0",
    "@types/glob": "^7.2.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "18.x",
    "typescript": "^5.3.3",
    "vscode-test": "^2.3.1"
  }
}